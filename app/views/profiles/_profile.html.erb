<div id="<%= dom_id profile %>">
  
  <p>
  <strong>Full Name:</strong>
  <%= profile.fullName %>
  </p>

  <p>
    <strong>Address One:</strong>
    <%= profile.addressOne %>
  </p>

  <p>
    <strong>Address Two:</strong>
    <%= profile.addressTwo %>
  </p>

  <p>
    <strong>Zip Code:</strong>
    <%= profile.zipcode %>
  </p>

  <p>
    <strong>City:</strong>
    <%= profile.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= profile.state %>
  </p>

</div>
