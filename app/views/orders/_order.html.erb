<div id="<%= dom_id order %>">
  <p>
    <strong>Gallons Requested:</strong>
    <%= order.gallonsRequested %>
  </p>

  <p>
    <strong>Date Ordered:</strong>
    <%= order.dateOrdered %>
  </p>

  <p>
    <strong>Profit Margin:</strong>
    <%= order.profitMargin %>
